{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "shell:default",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-write-file",
    "fs:allow-write",
    "fs:allow-temp-write",
    "fs:allow-download-write",
    "store:default",
    "dialog:default",
    "dialog:allow-save",
    "core:path:default",
    "core:window:allow-create",
    "core:webview:allow-create-webview-window"
  ]
}
